{
  "name": "<%= _.slugify(appname) %>",
  "version": "1.0.0",
  "description": "A generator for Yeoman",
  "keywords": [
    "yeoman-generator"
  ],
  "homepage": "<%= githubUrl %>/<%= _.slugify(appname) %>",
  "bugs": "<%= githubUrl %>/<%= _.slugify(appname) %>/issues",
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>",
    "url": "<%= githubUrl %>"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= _.slugify(appname) %>.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.spec.coffee"
  },
  "dependencies": {
    "yeoman-generator": "^0.14.2",
    "coffee-script": "^1.7.1"
  },
  "devDependencies": {
    "mocha": "^1.18.2"
  },
  "peerDependencies": {
    "yo": "^1.0.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
