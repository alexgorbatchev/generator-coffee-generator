{
  "name": "<%= _.slugify(appname) %>",
  "version": "1.0.0",
  "description": "A generator for Yeoman",
  "keywords": [
    "yeoman-generator"
  ],
  "homepage": "<%= githubUrl %>/<%= _.slugify(appname) %>",
  "bugs": "<%= githubUrl %>/<%= _.slugify(appname) %>/issues",
  "author": {
    "name": "<%= realname %>",
    "url": "<%= githubUrl %>"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= _.slugify(appname) %>.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.spec.coffee"
  },
  "dependencies": {
    "yeoman-generator": "^0.22.5",
    "github": "^0.2.4",
    "coffee-errors": "^0.8.6",
    "coffee-script": "^1.10.0"
  },
  "devDependencies": {
    "mocha": "^1.18.2",
    "rimraf": "^2.2.8"
  },
  "peerDependencies": {
    "yo": "^1.7.0"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=2.10.0"
  },
  "license": "MIT"
}
